{
  "name": "zeroknowledgeproofs",
  "version": "1.0.0",
  "description": "Zero-Knowledge Proof Age Verification - Groth16 & PLONK implementations",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "circom circuits/age_check.circom --r1cs --wasm --sym -o circuits/compiled -l node_modules",
    "setup:groth16": "snarkjs groth16 setup circuits/compiled/age_check.r1cs circuits/compiled/pot12_final.ptau circuits/compiled/age_check_groth16.zkey",
    "setup:plonk": "snarkjs plonk setup circuits/compiled/age_check.r1cs circuits/compiled/pot12_final.ptau circuits/compiled/age_check_plonk.zkey",
    "export:vkey:groth16": "snarkjs zkey export verificationkey circuits/compiled/age_check_groth16.zkey circuits/compiled/verification_key_groth16.json",
    "export:vkey:plonk": "snarkjs zkey export verificationkey circuits/compiled/age_check_plonk.zkey circuits/compiled/verification_key_plonk.json",
    "download:ptau": "curl -L https://hermez.s3-eu-west-1.amazonaws.com/powersOfTau28_hez_final_12.ptau -o circuits/compiled/pot12_final.ptau"
  },
  "keywords": [
    "zero-knowledge",
    "zkp",
    "groth16",
    "plonk",
    "snarkjs",
    "circom",
    "age-verification"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "snarkjs": "^0.7.5",
    "circomlib": "^2.0.5"
  }
}
